{
    "name": "drupal/quiz_maker",
    "license": "GPL-2.0-or-later",
    "type": "drupal-module",
    "description": "Provide functionality to Create engaging quizzes.",
    "support": {
      "issues": "https://drupal.org/project/issues/quiz_maker",
      "source": "https://drupal.org/project/quiz_maker"
    },
    "authors": [
        {
            "name": "Roman Chekhaniuk",
            "email": "roman.chekhaniuk@lembergsolutions.com"
        }
    ],
    "require": {
      "php": ">=8.1",
      "drupal/entity_browser": "^2.10",
      "drupal/inline_entity_form": "^3.0@RC || ^1.0@RC",
      "drupal/views_bulk_operations": "^4.2",
      "drupal/field_group": "^3.4",
      "drupal/entity": "^1.4",
      "drupal/entity_reference_revisions": "^1.11",
      "drupal/state_machine": "^1.10"
    },
    "require-dev": {
        "mglaman/phpstan-drupal": "^1.2",
        "phpro/grumphp": "^2.4",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "drupal/coder": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
